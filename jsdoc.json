{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "template": "./node_modules/tidy-jsdoc",
      "prism-theme": "prism-custom",
      "encoding": "utf8",
      "destination": "./docs/",
      "recurse": true,
      "sort": false
    },
    "plugins": ["plugins/markdown", "plugins/summarize"],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "metadata": {
      "title": "React Time Tracker Documentation"
    }
  }